.register_wrap{
	background-color: #008DE1;
}
.register_bg{
	background: url(../img/register_bg.png) no-repeat;
	background-size: 100%;
	overflow: hidden;
}
.register_box{
	width: 25%;
	border: 1px solid silver;
	margin: 3% 0 3% 60%;
	box-sizing: border-box;
	padding: 1%;
	background-color: #fff;
	overflow: hidden;
}
.register_box_input{
	width: 100%;
	border: 1px solid #E6E6E6;
	height: 2.8rem;
	background-color: #F9F9F9;
	position: relative;
}
.register_box_input input{
	box-sizing: border-box;
	width: 100%;
	height: 100%;
	border: none;
	padding-left: 1rem;
}
.register_box_input img{
	height: 1rem;
	width: 1rem;
	position: absolute;
	right: 4%;
	top: 30%;
}
.register_box_font{
	color: red;
	line-height: 2rem;
}
.register_box_font img{
	height: 1rem;
	width: 1rem;
	display: inline-block;
	vertical-align: text-top;
	margin-top: 0.2rem;
}
.register_box_input2{
	border: 0;
	overflow: hidden;
}
.register_box_input2 input{
	border: 1px solid #E6E6E6;
	width: 35%;
	float: left;
}
.register_box_yzm{
	box-sizing: border-box;
	float: right;
	width: 64%;
	height: 100%;
	overflow: hidden;
}
.yzm{
	box-sizing: border-box;
	width: 50%;
	height: 100%;
	border: 1px solid #E6E6E6;
	float: left;
}
.register_box_yzm a{
	float: right;
	color: #F19459;
	line-height: 2.8rem;
}
.register_box_input{
	position: relative;
}
.register_box_input3 a{
	position: absolute;
	display: inline-block;	
	top: 8%;
	right: 1%;
	color: #fff;	
	background-color: #008DE1;
	padding: 0.5rem 1rem;
	border-radius: 0.3rem;
}
.register_box_button{
	text-align: center;
	float: left;
	width: 50%;
	margin-top: 6%;
}
.register_box_button a{
	color: #fff;	
	background-color: #008DE1;
	padding: 0.65rem 1.8rem;
	border-radius: 0.3rem;
}
.register_box_button1 a{
	background-color: #EC6A17;
}
/*login*/
.login_font{
	padding-left: 1%;
	font-size: 2.3rem;
	margin-bottom: 2%;
	color: #666666;
	overflow: hidden;
}
.login_font img{
	margin-top: 1rem;
	width: 1rem;
	height: 1rem;
	float: right;
}
.login_font a{
	line-height: 3rem;
	font-size: 1.3rem;
	float: right;	
	color:#37A6E7;
}
.login_forget{
	line-height: 1rem;
	overflow: hidden;
}
.login_forget input{
	display: inline-block;
}
.login_forget a{
	float: right;
	color: #F08643;
}
.login_font1{
	font-size: 1.3rem;
	width: 100%;
	margin-top: 25%;
	text-align: center;
	color: #969696;
	background: url(../img/login_spl.png) no-repeat;
	background-position: center;
	background-size: 100%;
}
.login_logo{
	width: 25%;
	margin: 5% auto;
	overflow: hidden;
}
.login_logo a:last-child{
	float: right;
}
@media only screen and (max-width: 1110px) {
	.register_box{
		width: 35%;
		margin: 3% 0 3% 60%;
		padding: 1%;
	}
}
@media only screen and (max-width: 780px) {
	.register_box{
		width: 50%;
		margin: 3% 0 3% 45%;
		padding: 1%;
	}
}
@media only screen and (max-width: 580px) {
	.register_box{
		width: 78%;
		margin: 30% 11% 10% 11%;
		padding: 1%;
	}
	.login_font a{
		line-height: 3rem;
		font-size: 1rem;
	}
	.login_font{
		line-height: 3rem;
		font-size: 2rem;
		margin-bottom: 1%;
	}
	.login_logo{
		width: 30%;
	}
}